.myserve {
  position: absolute;
  background: #f2f2f2;
  min-height: 100%;
  width: 100%;
  overflow: hidden;
}

.header {
  margin-top: 15px;
}

.content {
  background-color: white;
  margin-top: 15px;
}

.myserve .addbtn {
  color: white;
  margin-top: 20px;
  width: 90%;
  height: 44px;
  margin-bottom: 40px;
  background-color: #0af;
}

.cell-item {
  background-color: white;
}

.image {
  width: 44px;
  height: 44px;
  top: 10px;
  left: 15px;
  position: relative;
  display: flex;
  border-radius: 30px;
}

.title {
  position: relative;
  color: #353535;
  text-align: left;
  left: 74px;
  top: -33px;
  font-size: 16px;
  height: 16px;
  line-height: 16px;
}

.subtitle {
  position: relative;
  text-align: left;
  color: #888;
  top: -23px;
  left: 74px;
  max-width: 200px;
  font-size: 14px;
  height: 16px;
  line-height: 16px;
  overflow: hidden;
}
.cell_line {
  position: relative;
  left: 20px;
  height: 1px;
  margin-top: -10px;
  background-color: #dfdfdf;
}
.widget__arrow{
   position: absolute;
    margin-left: 90%;
    margin-top:-50px;
    right: 15px;
    width: 16px;
    height: 16px;
}
.cell_header-line {
  position: relative;
  left: 20px;
  height: 1px;
  margin-top: -10px;
